{
  "name": "konnors-ninja-keys",
  "version": "1.10.0",
  "description": "Konnors Ninja Keys",
  "main": "src/ninja-keys.js",
  "module": "src/ninja-keys.js",
  "type": "module",
  "types": "./src/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./src/index.js"
    },
    "./ninja-keys.js": {
      "import": "./src/ninja-keys.js"
    },
    "./ninja-header.js": {
      "import": "./src/ninja-header.js"
    }
  },
  "scripts": {
    "start:docs": "cd docs && bin/bridgetown start",
    "setup": "pnpm install && cd docs && pnpm install && bundle install",
    "build:docs": "cd docs && pnpm run build && cd - && pnpm run build:api",
    "build:api": "typedoc --out ./docs/output/references/api/ ./exports/**/*.js",
    "build": "npm run clean && tsc && npm run build:cem",
    "watch:cem": "custom-elements-manifest analyze --litelement --watch",
    "build:cem": "custom-elements-manifest analyze --litelement",
    "clean": "rimraf -g \"./src/**/*.d.ts\" \"./internal/**/*.d.ts\"",
    "build:watch": "tsc --watch",
    "lint": "npm run lint:lit-analyzer && npm run lint:eslint",
    "lint:eslint": "eslint 'src/**/*.ts'",
    "lint:lit-analyzer": "lit-analyzer",
    "format": "prettier \"**/*.{cjs,html,js,json,md,ts}\" --ignore-path ./.eslintignore --write",
    "analyze": "cem analyze --litelement --globs \"src/**/*.ts\"",
    "analyze:watch": "cem analyze --litelement --globs \"src/**/*.ts\" --watch",
    "serve": "wds --root-dir=. --node-resolve --watch",
    "serve:prod": "MODE=prod npm run serve",
    "bundle": "npm run build && rollup -c --file dist/ninja-keys.bundled.js",
    "checksize": "rollup -c ; cat dist/ninja-keys.bundled.js | gzip -9 | wc -c ; rm dist/ninja-keys.bundled.js",
    "prepublish": "npm run clean && npm run build",
    "deploy:patch": "npm run clean && npm run build && npm version patch && clean-publish",
    "deploy:minor": "npm run clean && npm run build && npm version minor && clean-publish",
    "deploy:major": "npm run clean && npm run build && npm version major && clean-publish",
    "deploy-test": "rimraf package && clean-publish --without-publish --temp-dir package"
  },
  "files": [
    "src",
    "types",
    "custom-elements.json",
    "README.md",
    "LICENSE",
    "package.json",
    "CHANGELOG.md"
  ],
  "keywords": [
    "web-components",
    "lit-element",
    "typescript",
    "react",
    "vue",
    "javascript",
    "keyboard",
    "hotkeys",
    "shortcut",
    "keypress",
    "hotkey"
  ],
  "author": "Konnor Rogers <konnor5456@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/konnorrogers/konnors-ninja-keys",
  "repository": {
    "type": "git",
    "url": "git@github.com:konnorrogers/konnors-ninja-keys.git"
  },
  "clean-publish": {
    "files": [
      "dev",
      "docs",
      "rollup.config.js",
      "web-dev-server.config.mjs"
    ]
  },
  "dependencies": {
    "@material/mwc-icon": "^0.25.3",
    "@open-wc/scoped-elements": "^2.1.5",
    "hotkeys-js": "3.10.2",
    "lit": "^2.7.4",
    "web-component-define": "^2.0.10"
  },
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "^0.5.3",
    "@open-wc/testing": "^3.1.8",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-replace": "^3.0.0",
    "@types/node": "^20.2.4",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.30.0",
    "@web/dev-server": "0.1.25",
    "@web/dev-server-rollup": "^0.3.9",
    "@web/test-runner": "^0.16.1",
    "@web/test-runner-playwright": "^0.10.0",
    "@webcomponents/webcomponentsjs": "^2.6.0",
    "clean-publish": "3.4.2",
    "eslint": "^7.32.0",
    "lit-analyzer": "^1.1.10",
    "konnors-ninja-keys": "link:./",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.28.2",
    "rollup-plugin-minify-html-literals": "1.2.6",
    "rollup-plugin-summary": "^1.2.3",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "~5.0.0"
  },
  "customElements": "custom-elements.json"
}
